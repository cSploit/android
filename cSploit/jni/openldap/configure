#include <sys/types.h>
#include <regex.h>
static char *pattern, *string;
main()
{
	int rc;
	regex_t re;

	pattern = "^A";

	if(regcomp(&re, pattern, 0)) {
		return -1;
	}

	string = "ALL MATCH";

	rc = regexec(&re, string, 0, (void*)0, 0);

	regfree(&re);

	return rc;
}
