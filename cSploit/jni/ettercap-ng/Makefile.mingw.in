#
# Create dependencies
#
depend: $(DEPEND_REQ)
	$(CC) -MM $(CFLAGS) $(SOURCE) $(EF_SOURCE) $(EL_SOURCE)| \
	  sed -e 's/^\([a-zA-Z0-9_-]*\.o:\)/$$(OBJ_DIR)\/\1/' > .depend
ifeq ($(USE_PLUGIN),1)
	$(CC) -MM $(CFLAGS) -DBUILDING_PLUGIN $(PLUGIN_SRC) | \
	  sed -e 's/^\([a-zA-Z0-9_-]*\)\.o:/lib\/ec_\1\.dll:/' >> .depend
endif

-include .depend
