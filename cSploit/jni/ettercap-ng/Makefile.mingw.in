  SOURCE += $(addprefix src/interfaces/curses/widgets/, wdg.c wdg_compound.c wdg_debug.c \
            wdg_dialog.c wdg_dynlist.c wdg_error.c wdg_file.c wdg_input.c wdg_list.c \
